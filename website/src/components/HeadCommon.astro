---
// Import the global.css file here so that it is included on
// all pages through the use of the <HeadCommon/> component.
import '../main.css';
---

<!-- Global Metadata -->
<meta charset="utf-8" />
<meta name="viewport" content="minimum-scale=1, width=device-width" />
<meta name="theme-color" content="#156A63" />
<link rel="icon" type="image/webp" href="/images/logo.webp" />
<meta name="generator" content={Astro.generator} />

<!-- Preloads -->
<link rel="preload" as="font" type="font/ttf" href="/fonts/Lexend_Deca/LexendDeca-VariableFont_wght.ttf" crossorigin/>
<link rel="preload" as="font" type="font/ttf" href="/fonts/Noto_Sans_Mono/NotoSansMono-VariableFont_wdth,wght.ttf" crossorigin/>
<link rel="preload" as="font" type="font/ttf" href="/fonts/Orbitron/Orbitron-VariableFont_wght.ttf" crossorigin/>
